    "encoding": {
      "type": "string"
    },
    "tileSize": {
      "type": "integer"
    },
    "layoutExtent": {
      "type": "object",
      "properties": {
        "xmin": {
          "type": "number"
        },
        "ymin": {
          "type": "number"
